shortcut-add aw=https://wiki.archlinux.org/index.php?title=Special%3ASearch&search=$0&go=Go
shortcut-add abbs=https://startpage.com/do/metasearch.pl?query=site:bbs.archlinux.org+$0
shortcut-add se=http://stackexchange.com/search?q=$0
shortcut-add github=https://github.com/search?q=$0
shortcut-add w=https://secure.wikimedia.org/wikipedia/en/w/index.php?title=Special%%3ASearch&search=$0&go=Go
shortcut-add yt=http://www.youtube.com/results?search_query=$0&sm=3
shortcut-add noaa=http://forecast.weather.gov/zipcity.php?inputstring=$0
shortcut-add oed=http://www.oed.com/search?searchType=dictionary&q=$0
shortcut-default sp

