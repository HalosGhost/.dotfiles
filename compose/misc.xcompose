#<- Miscellaneous ->
<Multi_key> <asterisk> <period>                 : "•"   U2022   # Bullet
<Multi_key> <asterisk> <minus>                  : "⁃"   U2043   # Hypen Bullet
<Multi_key> <minus> <bar>                       : "†"   U2020   # Dagger
<Multi_key> <bar> <minus> <minus>               : "‡"   U2021   # Double Dagger
<Multi_key> <asterisk> <asterisk> <asterisk>    : "⁂"   U2042   # Asterism
<Multi_key> <asciicircum> <asciicircum>         : "‸"   U2038   # Caret
<Multi_key> <o> <o>                             : "°"   U00B0   # Degree
<Multi_key> <quotedbl> <quotedbl>               : "〃"  U3003   # Ditto
<Multi_key> <N> <o>                             : "№"   U2116   # Numero
<Multi_key> <a> <t> <h>                         : "ª"   U00AA   # Feminine Ordinal
<Multi_key> <o> <t> <h>                         : "º"   U00BA   # Masculine Ordinal
<Multi_key> <underscore> <space>                : "␣"   U2423   # Open box

<Multi_key> <percent> <o>                       : "‰"   U2030   # Permille
<Multi_key> <percent> <percent>                 : "‱"   U2031   # Basis Point

<Multi_key> <P> <P>                             : "¶"   U00B6   # Pilcrow
<Multi_key> <s> <e> <c>                         : "§"   U00A7   # Section

<Multi_key> <o> <c>                             : "©"   U00A9   # Copyright
<Multi_key> <t> <m>                             : "™"   U2122   # Trade Mark

<Multi_key> <equal> <equal> <greater>           : "⇒"   U21D2   # Right Double Arrow
<Multi_key> <equal> <equal> <less>              : "⇐"   U21D0   # Left Double Arrow

<Multi_key> <t> <r> <i>                         : "▲"   U25B2   # Up-Pointing Black Triangle
<Multi_key> <i> <r> <t>                         : "▼"   U25BC   # Down-Pointing Black Triangle

<Multi_key> <less> <3>                          : "❤"   U2764   # Black Heart
<Multi_key> <p> <e> <a> <c> <e>                 : "☮"   U262E   # Peace Sign
<Multi_key> <i> <n> <f>                         : "∞"   U221E   # Infinity Symbol

<Multi_key> <h> <r> <t>                         : "♡"   U2661   # White Heart Suit
<Multi_key> <c> <l> <b>                         : "♣"   U2663   # Black Club Suit
<Multi_key> <d> <m> <n> <d>                     : "♢"   U2662   # White Diamond Suit
<Multi_key> <s> <p> <d>                         : "♠"   U2660   # Black Spade Suit

#<- Modifier keys ->
<Multi_key> <a> <l> <t>                         : "⌥"   U2325   # Alternate
<Multi_key> <s> <h> <f> <t>                     : "⇧"   U21E7   # Shift

#<- Currency ->
<Multi_key> <e> <u> <r>                         : "€"   U20AC   # Euro
<Multi_key> <g> <b> <p>                         : "£"   U00A3   # British Pound Sterling
